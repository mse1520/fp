{
  "name": "underbarjs",
  "version": "2.0.0-beta.6",
  "author": "kyh",
  "license": "MIT",
  "description": "",
  "main": "lib/cjs/entry.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev concurrently \"npm run watch:generate-index\" \"webpack serve\"",
    "build": "npm run build-cjs & npm run build-mjs & webpack",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "generate-index": "npm run ts-node ./scripts/indexGenerator.ts",
    "watch:generate-index": "nodemon -i src/index.ts -w src/ -e ts -x npm run generate-index",
    "build-cjs": "tsc -p ./tsconfig.cjs.json & tsc-alias -p ./tsconfig.cjs.json",
    "build-mjs": "tsc -p ./tsconfig.mjs.json & tsc-alias -p ./tsconfig.mjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mse1520/underbarjs.git"
  },
  "bugs": {
    "url": "https://github.com/mse1520/underbarjs/issues"
  },
  "homepage": "https://github.com/mse1520/underbarjs#readme",
  "dependencies": {
    "core-js": "^3.25.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/node": "^18.7.14",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.9.0"
  },
  "files": [
    "lib",
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./lib/cjs/entry.js",
      "import": "./lib/mjs/entry.js"
    }
  }
}
